<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.icbc.dataAccess.dto.UserDto" table="user">
		<id name="id" column="id">
			<generator class="native"></generator>	
		</id>
		<property name="username"></property>
		<property name="password"></property>
		<property name="email"></property>
		<property name="phone"></property>
		<property name="institution"></property>
		<property name="type"></property>
		<property name="gender"></property>
		<property name="age"></property>
		<property name="status"></property>
		<property name="province"></property>
		<property name="city"></property>
		<property name="district"></property>
		<property name="street"></property>
		<property name="community"></property>
		<property name="unit"></property>
		<property name="lon"></property>
		<property name="lat"></property>	
	</class>
</hibernate-mapping>